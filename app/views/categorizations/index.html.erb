<h1>Categorizations - Index</h1>
<%= render 'form' %>
<table>
    <tr>
        <th>id</th>
        <th>poi_id</th>
        <th>interest_id</th>
        <th>action</th>
    </tr>
<% @categorizations.each do |categorization| %>
    <tr>
        <td><%= categorization.id %></td>
        <td><%= categorization.poi_id %></td>
        <td><%= categorization.interest_id %></td>
        <td><%= link_to 'Delete', categorization, :method => :delete %> | <%= link_to 'Edit', edit_categorization_path(categorization) %></td>
    </tr>
<% end %>
</table>

