<%= form_for @guest do |f| %>
    <%= f.label :first_name %>
    <%= f.text_field :first_name %>
    <%= f.label :last_name %>
    <%= f.text_field :last_name %>
    <%= f.label :birthday %>
    <%= f.text_field :birthday %>
    <%= f.label :current_city %>
    <%= f.text_field :current_city %>
    <%= f.label :tracker_id %>
    <%= f.text_field :tracker_id %>
  <%= f.submit %>
<% end %>