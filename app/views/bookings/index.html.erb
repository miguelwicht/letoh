<h1>Bookings - Index</h1>
<%= render 'form' %>
<table>
    <tr>
        <th>id</th>
        <th>from</th>
        <th>until</th>
    </tr>
<% @bookings.each do |booking| %>
    <tr>
        <td><%= booking.id %></td>
        <td><%= booking.from %></td>
        <td><%= booking.until %></td>
    </tr>
<% end %>
</table>

