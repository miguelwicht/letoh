<h1>Interests - Index</h1>
<%= render 'form' %>
<table>
    <tr>
        <th>id</th>
        <th>interest</th>
        <th>action</th>
    </tr>
<% @interests.each do |interest| %>
    <tr>
        <td><%= interest.id %></td>
        <td><%= interest.name %></td>
        <td><%= link_to 'Delete', interest, :method => :delete %> | <%= link_to 'Edit', edit_interest_path(interest) %></td>
    </tr>
<% end %>
</table>

