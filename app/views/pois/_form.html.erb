<%= form_for @poi do |f| %>
    <%= f.label :long %>
    <%= f.text_field :long %>
    <%= f.label :lat %>
    <%= f.text_field :lat %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <%= f.label :address %>
    <%= f.text_field :address %>
    <%= f.label :description %>
    <%= f.text_field :description %>
    <%= f.label :duration %>
    <%= f.text_field :duration %>
  <%= f.submit %>
<% end %>